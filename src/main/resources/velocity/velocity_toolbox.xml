<?xml version="1.0"?>

<!-- Like Pull-MVC to expose some util classes to vm -->

<toolbox>
    <tool>
        <key>link</key>
        <scope>request</scope>
        <class>org.apache.velocity.tools.generic.LinkTool</class>
    </tool>
    <tool>
        <key>stringUtils</key>
        <scope>application</scope>
        <class>com.alibaba.ydt.portal.util.StringUtils</class>
    </tool>
    <tool>
        <key>moduleTool</key>
        <scope>application</scope>
        <class>com.alibaba.ydt.portal.util.ModuleTool</class>
    </tool>
</toolbox>
