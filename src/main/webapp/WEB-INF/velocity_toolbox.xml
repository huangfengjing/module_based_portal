<?xml version="1.0"?>

<!-- Like Pull-MVC to expose some util classes to vm -->

<toolbox>
    <tool>
        <key>link</key>
        <scope>request</scope>
        <class>com.alibaba.ydt.portal.util.LinkTool</class>
    </tool>
    <tool>
        <key>stringUtils</key>
        <scope>application</scope>
        <class>com.alibaba.ydt.portal.util.StringUtils</class>
    </tool>
    <tool>
        <key>commonTool</key>
        <scope>application</scope>
        <class>com.alibaba.ydt.portal.util.CommonTool</class>
    </tool>
    <tool>
        <key>renderTool</key>
        <scope>request</scope>
        <class>com.alibaba.ydt.portal.util.RenderTool</class>
    </tool>
</toolbox>
